function loot_weapon_mod_add(argument0, argument1, argument2) //gml_Script_loot_weapon_mod_add
{
    if (argument0 == undefined)
        return false;
    if (argument1 == undefined)
        return false;
    if (argument2 == undefined)
        return false;
    if (argument2 == "handguard")
    {
        if (item_exists(loot_weapon_get_mod(argument0, "att_1")) || item_exists(loot_weapon_get_mod(argument0, "att_2")) || item_exists(loot_weapon_get_mod(argument0, "att_3")) || item_exists(loot_weapon_get_mod(argument0, "att_4")))
        {
            scr_draw_text_with_box("You have to uninstall the attachments on the handguard")
            return false;
        }
    }
    var _mod_container = variable_struct_get(argument0, "mods")
    if (!is_struct(_mod_container))
        return false;
    var _mod_item = loot_get_item(argument1)
    if (!(item_mod_fits_position(_mod_item, argument2)))
        return false;
    var _existing_mod = loot_mod_cont_get(_mod_container, argument2)
    if item_exists(_existing_mod)
    {
        if (!inventory_check_for_space(_existing_mod))
        {
            scr_draw_text_with_box("Not enough inventory space!")
            return false;
        }
    }
    if (argument2 == "magazine")
    {
        if (!(gml_Script_loot_action_unload_now(argument0, true)))
            return false;
    }
    loot_mod_cont_set(_mod_container, argument2, _mod_item)
    inventory_and_stash_remove_item(_mod_item, 1)
    if item_exists(_existing_mod)
        inventory_add_item(_existing_mod, 1)
    return true;
}

